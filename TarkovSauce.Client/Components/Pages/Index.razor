@page "/"

<PageTitle>Tarkov Sauce</PageTitle>
@if (StateContainer.State.Value == Services.State.Home)
{
    <Home />
}
else if (StateContainer.State.Value == Services.State.FleaSales)
{
    <FleaSales />
}
else if (StateContainer.State.Value == Services.State.ComponentTests)
{
    <ComponentTests />
}
else if (StateContainer.State.Value == Services.State.Settings)
{
    <SettingsPage />
}
else if (StateContainer.State.Value == Services.State.Tasks)
{
    <TasksPage />
}
else if(StateContainer.State.Value == Services.State.Logs)
{
    <LogsPage />
}
else if(StateContainer.State.Value == Services.State.Raid)
{
    <RaidPage />
}